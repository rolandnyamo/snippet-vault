{
  "name": "snippet-vault",
  "productName": "Snippet Vault",
  "version": "1.1.0",
  "description": "A lightweight, entirely-offline desktop utility for storing and semantically searching code snippets, Kusto queries, and URLs using TensorFlow.js",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "webpack --config webpack.config.cjs",
    "build:watch": "webpack --watch --config webpack.config.cjs",
    "build:full": "webpack --config webpack.config.cjs",
    "download-models": "node scripts/download-model.js",
    "prebuild": "echo 'Skipping model download during build - models will download on first run'",
    "start": "npm run build && electron-forge start",
    "package": "electron-forge package",
    "package:all": "npm run package:mac && npm run package:win && npm run package:linux",
    "package:mac": "electron-forge package --platform=darwin --arch=x64",
    "package:win": "electron-forge package --platform=win32 --arch=x64",
    "package:linux": "electron-forge package --platform=linux --arch=x64",
    "make": "electron-forge make",
    "make:all": "npm run make:mac && npm run make:win && npm run make:linux",
    "make:mac": "electron-forge make --platform=darwin --arch=x64",
    "make:win": "electron-forge make --platform=win32 --arch=x64",
    "make:linux": "electron-forge make --platform=linux --arch=x64",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "test": "npx jest",
    "commit": "cz",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "kusto",
    "snippets",
    "code",
    "vault",
    "search"
  ],
  "author": {
    "name": "roland nyamoga",
    "email": "rolandn1844@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/rolandnyamo/snippet-vault",
  "repository": {
    "type": "git",
    "url": "https://github.com/rolandnyamo/snippet-vault.git"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@babel/preset-react": "^7.27.1",
    "@lancedb/lancedb": "^0.21.2",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs-backend-cpu": "^3.21.0",
    "@tensorflow/tfjs-converter": "^3.21.0",
    "@tensorflow/tfjs-core": "^3.21.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "apache-arrow": "^18.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.28.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-wix": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^24.2.0",
    "babel-jest": "^30.0.5",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "commitizen": "^4.3.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "37.2.5",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^8.0.3",
    "jest": "^30.0.5",
    "semantic-release": "^22.0.12",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  }
}
